{
    "name": "mmaerov/user-permissions",
    "type": "package",
    "description": "The skeleton application for the Laravel framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mayorov Max",
            "email": "mmaerov@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\Debugbar\\UserPermissionServiceProvider"
            ]
        }
    }
}
